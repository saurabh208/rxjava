  "homePhone": "string",
  "mobilePhone": "string",
  "nextOfKin_lastname": "string",
  "nextOfKin_firstname": "string",
  "serviceAddress": "string",
  "gender": "MALE",
  "allergies": [
    "string"
  ],
  "allergiesReaction": [
    "string"
  ],
  "medicationList": [
    "string"
