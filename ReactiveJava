#Rxjava#SpringData
docker run -d --name fastapi-mongo -p 27017:27017 mongo:4.4
gradle build -p REACTIVEJAVA/Reactive-RxJava-Data/
java -cp REACTIVEJAVA/Reactive-RxJava-Data/build/libs/patient-service-1.0.0-SNAPSHOT.jar org.springframework.boot.loader.JarLauncher

gradle build -p REACTIVEJAVA/Reactive-RxJava-Data/ && java -jar REACTIVEJAVA/Reactive-RxJava-Data/build/libs/* &
curl http://localhost:8084/actuator/loggers/com.stillwaters.medconnect
curl -i -X POST -H 'Content-Type: application/json' -d '{"configuredLevel": "DEBUG"}' http://localhost:8084/actuator/loggers/com.stillwaters.medconnect
http://localhost:7002/openapi/webjars/swagger-ui/index.html?configUrl=/openapi/v3/api-docs/swagger-config
http -v http://localhost:7002/patient/1234
body='{
  "medicalRecordNumber": "1234",
  "firstName": "Srinivasa",
  "lastName": "Ramanujam",
